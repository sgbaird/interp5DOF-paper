@article{taoFastNumericalSolver2019,
  title = {A Fast Numerical Solver for Local Barycentric Coordinates},
  author = {Tao, Jiong and Deng, Bailin and Zhang, Juyong},
  year = {2019},
  volume = {70},
  pages = {46--58},
