@article{wareThermodynamicStabilityKinematic2020a,
  ids = {Ware2020c,wareThermodynamicStabilityKinematic2020},
  title = {Thermodynamic Stability and Kinematic Accessibility of Curved Grain Boundaries in Directionally Solidified Bicrystals},
