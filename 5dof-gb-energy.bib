@article{al-mohyComplexStepApproximation2010,
  title = {The Complex Step Approximation to the Fr\'echet Derivative of a Matrix Function},
  author = {{Al-Mohy}, Awad H. and Higham, Nicholas J.},
  year = {2010},
  volume = {53},
  pages = {133--148},
  issn = {10171398},
  doi = {10.1007/s11075-009-9323-y},
  abstract = {We show that the Fr\'echet derivative of a matrix function f at A in the direction E, where A and E are real matrices, can be approximated by Im f(A + ihE)/h for some suitably small h. This approximation, requiring a single function evaluation at a complex argument, generalizes the complex step approximation known in the scalar case. The approximation is proved to be of second order in h for analytic functions f and also for the matrix sign function. It is shown that it does not suffer the inherent cancellation that limits the accuracy of finite difference approximations in floating point arithmetic. However, cancellation does nevertheless vitiate the approximation when the underlying method for evaluating f employs complex arithmetic. The ease of implementation of the approximation, and its superiority over finite differences, make it attractive when specialized methods for evaluating the Fr\'echet derivative are not available, and in particular for condition number estimation when used in conjunction with a block 1-norm estimation algorithm. \textcopyright{} Springer Science + Business Media, LLC 2009.},
  file = {C\:\\Users\\sterg\\Zotero\\storage\\5J3G2PTU\\Al-Mohy_Higham_2010_The complex step approximation to the fréchet derivative of a matrix function.pdf;C\:\\Users\\sterg\\Zotero\\storage\\S3SHH295\\Al-Mohy, Higham - 2010 - The complex step approximation to the fréchet derivative of a matrix function(6).pdf;C\:\\Users\\sterg\\Zotero\\storage\\TVRL5WW5\\Al-Mohy_Higham_2010_The complex step approximation to the fréchet derivative of a matrix function.pdf;C\:\\Users\\sterg\\Zotero\\storage\\U3JN7AK3\\Al-Mohy_Higham_2010_The complex step approximation to the fréchet derivative of a matrix function.pdf;C\:\\Users\\sterg\\Zotero\\storage\\XLPQUMJU\\Al-Mohy_Higham_2010_The complex step approximation to the fréchet derivative of a matrix function.pdf},
  journal = {Numerical Algorithms},
  keywords = {Block 1-norm estimator,Complex arithmetic,Complex step approximation,Condition number estimation,Finite difference,Fréchet derivative,Matrix function,Matrix sign function},
  number = {1}
}

@article{albertGaussianProcessesData2019,
  title = {Gaussian {{Processes}} for {{Data Fulfilling Linear Differential Equations}}},
  author = {Albert, Christopher G.},
  year = {2019},
  volume = {33},
  pages = {5},
  issn = {2504-3900},
  doi = {10.3390/proceedings2019033005},
  abstract = {A method to reconstruct fields, source strengths and physical parameters based on Gaussian process regression is presented for the case where data are known to fulfill a given linear differential equation with localized sources. The approach is applicable to a wide range of data from physical measurements and numerical simulations. It is based on the well-known invariance of the Gaussian under linear operators, in particular differentiation. Instead of using a generic covariance function to represent data from an unknown field, the space of possible covariance functions is restricted to allow only Gaussian random fields that fulfill the homogeneous differential equation. The resulting tailored kernel functions lead to more reliable regression compared to using a generic kernel and makes some hyperparameters directly interpretable. For differential equations representing laws of physics such a choice limits realizations of random fields to physically possible solutions. Source terms are added by superposition and their strength estimated in a probabilistic fashion, together with possibly unknown hyperparameters with physical meaning in the differential operator.},
  archivePrefix = {arXiv},
  eprint = {1909.03447},
  eprinttype = {arxiv},
  file = {C\:\\Users\\sterg\\Zotero\\storage\\8WBV8KMM\\Albert_2019_Gaussian Processes for Data Fulfilling Linear Differential Equations.pdf;C\:\\Users\\sterg\\Zotero\\storage\\95XSPX28\\Albert_2019_Gaussian Processes for Data Fulfilling Linear Differential Equations.pdf;C\:\\Users\\sterg\\Zotero\\storage\\9ASCQ7RL\\Albert_2019_Gaussian Processes for Data Fulfilling Linear Differential Equations.pdf;C\:\\Users\\sterg\\Zotero\\storage\\A4ZHC487\\Albert_2019_Gaussian Processes for Data Fulfilling Linear Differential Equations.pdf;C\:\\Users\\sterg\\Zotero\\storage\\AZPRVIYU\\Albert_2019_Gaussian Processes for Data Fulfilling Linear Differential Equations.pdf;C\:\\Users\\sterg\\Zotero\\storage\\IHP6NWFE\\Albert_2019_Gaussian Processes for Data Fulfilling Linear Differential Equations.pdf},
  journal = {Proceedings},
  keywords = {field reconstruction,gaussian process regression,meshless,partial differential equations},
  number = {1}
}

