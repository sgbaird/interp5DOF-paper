@article{langerSphericalBarycentricCoordinates2006,
  title = {Spherical Barycentric Coordinates},
  author = {Langer, T and Belyaev, A and Seidel, Hans-Peter},
  year = {2006},
  pages = {81--88},
  issn = {1727-8384},
  url = {http://portal.acm.org/citation.cfm?id=1281957.1281968},
  abstract = {We develop spherical barycentric coordinates. Analogous to classical, planar barycentric coordinates that describe the positions of points in a plane with respect to the vertices of a given planar polygon, spherical barycentric coordinates describe the positions of points on a sphere with respect to the vertices of a given spherical polygon. In particular, we introduce spherical mean value coordinates that inherit many good properties of their planar counterparts. Furthermore, we present a construction that gives a simple and intuitive geometric interpretation for classical barycentric coordinates, like Wachspress coordinates, mean value coordinates, and discrete harmonic coordinates. One of the most interesting consequences is the possibility to construct mean value coordinates for arbitrary polygonal meshes. So far, this was only possible for triangular meshes. Furthermore, spherical barycentric coordinates can be used for all applications where only planar barycentric coordinates were available up to now. They include B\'ezier surfaces, parameterization, free-form deformations, and interpolation of rotations.},
  file = {C\:\\Users\\sterg\\Zotero\\storage\\59YDXXKZ\\Langer et al_2006_Spherical barycentric coordinates.pdf;C\:\\Users\\sterg\\Zotero\\storage\\7FRZ7Y9C\\Langer, Belyaev, Seidel - 2006 - Spherical barycentric coordinates(3).pdf;C\:\\Users\\sterg\\Zotero\\storage\\FIBIB8K6\\Langer, Belyaev, Seidel - 2006 - Spherical barycentric coordinates(3).pdf;C\:\\Users\\sterg\\Zotero\\storage\\XSMVWIGJ\\Langer et al_2006_Spherical barycentric coordinates.pdf},
