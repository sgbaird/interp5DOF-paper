  publisher = {{IEEE}},
  issn = {00189448},
  doi = {10.1109/TIT.2011.2173241},
  abstract = {Finding the sparsest solution to underdetermined systems of linear equations y = {$\varphi$}x is NP-hard in general. We show here that for systems with "typical"/"random" {$\varphi$}, a good approximation to the sparsest solution is obtained by applying a fixed number of standard operations from linear algebra. Our proposal, Stagewise Orthogonal Matching Pursuit (StOMP), successively transforms the signal into a negligible residual. Starting with initial residual r o = y, at the s-th stage it forms the "matched filter" {$\varphi$} Tr s-1, identifies all coordinates with amplitudes exceeding a specially chosen threshold, solves a least-squares problem using the selected coordinates, and subtracts the least-squares fit, producing a new residual. After a fixed number of stages (e.g., 10), it stops. In contrast to Orthogonal Matching Pursuit (OMP), many coefficients can enter the model at each stage in StOMP while only one enters per stage in OMP; and StOMP takes a fixed number of stages (e.g., 10), while OMP can take many (e.g., ).We give both theoretical and empirical support for the large-system effectiveness of StOMP.We give numerical examples showing that StOMP rapidly and reliably finds sparse solutions in compressed sensing, decoding of error-correcting codes, and overcomplete representation. \textcopyright{} 2011 IEEE.},
  file = {C\:\\Users\\sterg\\Zotero\\storage\\3A95BWVI\\Donoho et al_2012_Sparse solution of underdetermined systems of linear equations by stagewise.pdf;C\:\\Users\\sterg\\Zotero\\storage\\6ELXA65Q\\Donoho et al_2012_Sparse solution of underdetermined systems of linear equations by stagewise.pdf;C\:\\Users\\sterg\\Zotero\\storage\\FRM3PKYH\\Donoho et al_2012_Sparse solution of underdetermined systems of linear equations by stagewise.pdf;C\:\\Users\\sterg\\Zotero\\storage\\I6A3KF6Q\\Donoho et al_2012_Sparse solution of underdetermined systems of linear equations by stagewise.pdf;C\:\\Users\\sterg\\Zotero\\storage\\IUSUEIYH\\Donoho et al_2012_Sparse solution of underdetermined systems of linear equations by stagewise.pdf;C\:\\Users\\sterg\\Zotero\\storage\\JAKY2U29\\Donoho et al_2012_Sparse solution of underdetermined systems of linear equations by stagewise.pdf},
