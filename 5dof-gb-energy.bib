@article{allasiaHermiteBirkhoffInterpolation2018,
  title = {Hermite\textendash{{Birkhoff}} Interpolation on Scattered Data on the Sphere and Other Manifolds},
  author = {Allasia, Giampietro and Cavoretto, Roberto and De Rossi, Alessandra},
  year = {2018},
  volume = {318},
  pages = {35--50},
  publisher = {{Elsevier Inc.}},
  issn = {00963003},
  doi = {10.1016/j.amc.2017.05.018},
  abstract = {The Hermite\textendash Birkhoff interpolation problem of a function given on arbitrarily distributed points on the sphere and other manifolds is considered. Each proposed interpolant is expressed as a linear combination of basis functions, the combination coefficients being incomplete Taylor expansions of the interpolated function at the interpolation points. The basis functions depend on the geodesic distance, are orthonormal with respect to the point-evaluation functionals, and have all derivatives equal zero up to a certain order at the interpolation points. A remarkable feature of such interpolants, which belong to the class of partition of unity methods, is that their construction does not require solving linear systems. Numerical tests are given to show the interpolation performance.},
  archivePrefix = {arXiv},
  eprint = {1705.01032},
  eprinttype = {arxiv},
  file = {C\:\\Users\\sterg\\Zotero\\storage\\2SKP4DLK\\Allasia, Cavoretto, De Rossi - 2018 - Hermite–Birkhoff interpolation on scattered data on the sphere and other manifolds(3).pdf;C\:\\Users\\sterg\\Zotero\\storage\\IT9UTEQ9\\Allasia et al_2018_Hermite–Birkhoff interpolation on scattered data on the sphere and other.pdf;C\:\\Users\\sterg\\Zotero\\storage\\T4RB3PGF\\Allasia et al_2018_Hermite–Birkhoff interpolation on scattered data on the sphere and other.pdf;C\:\\Users\\sterg\\Zotero\\storage\\YN9Q8WSU\\Allasia, Cavoretto, De Rossi - 2018 - Hermite–Birkhoff interpolation on scattered data on the sphere and other manifolds(3).pdf},
  journal = {Applied Mathematics and Computation},
  keywords = {Arbitrarily distributed data,Hermite–Birkhoff interpolation,Meshfree methods,Multivariate approximation}
}

@article{anCorrelatingFiveParameter2018,
